   "source": [
    "# Function: kurtosis calculation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def k(x):\n",
    "    # kurtosis\n",
    "    # no correct for bias\n",
    "    mean = np.mean(x)\n",
    "    k4 = sum((x - mean)**4)/(len(x)-1)\n",
    "    k2 = sum((x - mean)**2)/(len(x)-1)\n",
    "    k = k4/k2**2\n",
    "    return k"
   ]
  },
