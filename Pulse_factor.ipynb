   "metadata": {},
   "outputs": [],
   "source": [
    "def pulse_factor(data):\n",
    "    max_data = max(data)\n",
    "    mean_data = np.mean(abs(data))\n",
    "    Pulse_factor = max_data/mean_data\n",
    "    return Pulse_factor"
   ]
